					<div class="details col-md-6">
            <br>

						<h3 class="product-title"><%= @product.title %></h3>
            <br>
            <br>

						<p class="product-description"><%= @product.description %></p>
            <div> 数量 : <%= @product.quantity %> </div>
            <br>
						<h4 class="price">￥ <%= @product.price %></h4>
						<div class="action">
              <button class="like btn btn-default">
                <% if @product.quantity.present? && @product.quantity > 0 %>
                  <%= link_to(content_tag(:i, '买买买', class: 'fa fa-shopping-cart'), add_to_cart_product_path(@product), :method => :post) %>
                <% else %>
                  车太帅，没货了
                <% end %>
              </button>
							<button class="like btn btn-default" type="button"><span class="fa fa-heart">收藏</span></button>
						</div>
					</div>
